{
  "name": "app-3",
  "version": "1.0.0",
  "description": "Rest API using Node.js, TypeScript, Express",
  "main": "server.ts",
  "scripts": {
    "clean": "rimraf build",
    "build": "pnpm clean && tsc",
    "start": "pnpm build && node -r newrelic ./build/server.js",
    "start:debug": "pnpm build && ts-node-dev --respawn --transpile-only --inspect=9231 --project tsconfig.json -r tsconfig-paths/register -r newrelic server.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "rest",
    "api",
    "restapi",
    "node",
    "nodejs",
    "crud"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.3.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "newrelic": "^11.8.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0"
  }
}
